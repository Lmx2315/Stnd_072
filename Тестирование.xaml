        <Label x:Name="label_Copy1" Content="Дбм" HorizontalAlignment="Left" Margin="135,241,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy2" Content="Гц" HorizontalAlignment="Left" Margin="135,213,0,0" VerticalAlignment="Top"/>
