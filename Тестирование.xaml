        <TextBox x:Name="LEVEL1" HorizontalContentAlignment="center" HorizontalAlignment="Left" Height="23" Margin="10,151,0,0" TextWrapping="Wrap" Text="+5" VerticalAlignment="Top" Width="120"/>
