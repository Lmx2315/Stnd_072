        <Label x:Name="label1_Copy5" Content="Длительность" HorizontalContentAlignment="center" HorizontalAlignment="Left" Margin="15,445,0,0" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Copy5" Content="мкС" HorizontalAlignment="Left" Margin="135,468,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button_Copy4" Content="Режим проверки подавления зеркальных каналов приема" HorizontalAlignment="Left" Margin="439,310,0,0" VerticalAlignment="Top" Width="570" Click="button_TST2_Click" FontSize="16"/>
        <Button x:Name="button_Copy5" Content="Режим проверки подавления канала прямого прохождения" HorizontalAlignment="Left" Margin="439,360,0,0" VerticalAlignment="Top" Width="570" Click="button_TST2_Click" FontSize="16"/>
