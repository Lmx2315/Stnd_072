        <Button x:Name="button_Copy1" Content="Режим проверки отклонения ∆fцап рабочей частоты" HorizontalAlignment="Left" Margin="439,160,0,0" VerticalAlignment="Top" Width="570" Click="button_TST2_Click" FontSize="16"/>
        <TextBox x:Name="FREQ1_Copy" HorizontalContentAlignment="center" HorizontalAlignment="Left" Height="23" Margin="15,363,0,0" TextWrapping="Wrap" Text="435000000" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1_Copy2" Content="Частота ЗС" HorizontalContentAlignment="center" HorizontalAlignment="Left" Margin="15,337,0,0" VerticalAlignment="Top" Width="120"/>
