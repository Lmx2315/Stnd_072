        <TextBox x:Name="ip_Copy" HorizontalContentAlignment="center" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" Text="192.168.10.3" VerticalAlignment="Top" Width="120"/>
